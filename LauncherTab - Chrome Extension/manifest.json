{
    "manifest_version": 3,
    "name": "LauncherTab",
    "version": "0.1.0",
    "description": "Copy anything, infinitely.",
    "author": "(ing) Studios Web Labs",
    "chrome_url_overrides": {
        "newtab": "newtab/newtab.html"
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "permissions": [
        "tabs",
        "storage",
        "favicon"
    ],
    "web_accessible_resources": [
        {
        "resources": ["_favicon/*"],
        "matches": ["<all_urls>"],
        "extension_ids": ["*"]
        }
    ],
    "icons": {
      "48": "assets/logo.png"
    }
}